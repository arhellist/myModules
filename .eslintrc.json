{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "comma-spacing": "off",
        "indent": "off",
        "no-console": "off",
        "import/extensions": "off",
        "import/first": "off",
        "linebreak-style": "off",
        "import/newline-after-import":"off",
        "eol-last":"off",
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ]
   }
}
